<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/fragment_find"
    android:background="?colorDefault">
    <LinearLayout
        android:id="@+id/app_top_space"
        style="@style/app_status_bar"
        android:orientation="horizontal"/>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="?colorPrimary"
        android:paddingStart="@dimen/dimenSpace"
        android:paddingEnd="@dimen/dimenSpace">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="48dp"
            android:gravity="center"
            android:layout_centerInParent="true"
            android:text="@string/fragment_find_title"
            android:textColor="?colorDefault"
            android:textSize="@dimen/dimenFontL"/>
    </RelativeLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:gravity="center">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textColor="?colorFont"
            android:textSize="@dimen/dimenFont"
            android:text="@string/error_incoming"/>
    </LinearLayout>
