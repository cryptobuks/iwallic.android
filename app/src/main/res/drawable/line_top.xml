<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item>
        <shape
            android:shape="rectangle">
            <stroke android:width="@dimen/dimenLine" android:color="?colorLine" />
            <solid android:color="?colorLine" />
        </shape>
    </item>

    <item android:top="@dimen/dimenLine" android:bottom="@dimen/dimenLine">
        <shape
            android:shape="rectangle">
            <stroke android:width="@dimen/dimenLine" android:color="?colorDefault" />
            <solid android:color="?colorDefault" />
        </shape>
    </item>

</layer-list>